Cell[TextData[{
 "We can now decrypt this text by reversing the cipher expression, i.e. if we \
have ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{"|", 
     RowBox[{"ap", "+", "b"}], 
     SubscriptBox["|", "m"]}]}], TraditionalForm]],ExpressionUUID->
  "81b1d47b-5187-44b2-8050-7ddde484166e"],
