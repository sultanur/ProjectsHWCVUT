88d9a499c8e7"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"b", "=", 
    RowBox[{"|", 
     RowBox[{"M", "-", 
      RowBox[{"a", " ", "T"}]}], 
     SubscriptBox["|", "26"]}]}], TraditionalForm]],ExpressionUUID->
  "4364f3dd-b271-46d2-9d50-f8b5871f9594"],
 "\n\t",
 Cell[BoxData[
  FormBox["H", TraditionalForm]],ExpressionUUID->
  "5976d844-d6c3-4883-a4f0-5fd54f07a975"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    RowBox[{
     RowBox[{"a", " ", "E"}], "+"}], "|", 
    RowBox[{"M", "-", 
     RowBox[{"a", " ", "T"}]}], 
    SubscriptBox["|", "26"], 
    SubscriptBox["|", "26"]}], TraditionalForm]],ExpressionUUID->
  "198f3cf1-c271-48dc-ac85-6eb4ffc3b605"]
}], "Text",
 CellChangeTimes->{{3.790878135668473*^9, 3.790878155299839*^9}, {
  3.790878216794581*^9, 
  3.790878217987424*^9}},ExpressionUUID->"04ddcc52-f6e4-4061-99aa-\
effe9597adf7"],

Cell[TextData[{
 "Because the reduction modulo 26 can be done at any time, we can rewrite:\n\t\
",
 Cell[BoxData[
  FormBox[
   RowBox[{"H", " "}], TraditionalForm]],ExpressionUUID->
  "d59fe7a7-5143-4482-bacd-18626aa2cd72"],
 "= ",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    RowBox[{
     RowBox[{"a", " ", 
      RowBox[{"(", " ", 
       RowBox[{"E", " ", "-", " ", "T"}], " ", ")"}]}], "+", "M"}], 
    SubscriptBox["|", "26"]}], TraditionalForm]],ExpressionUUID->
  "f63e2d5a-9722-4062-973a-2065e2f05b47"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", " ", "=", 
    RowBox[{"|", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"H", "-", "M"}], ")"}], "*", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"E", "-", "T"}], ")"}], 
       RowBox[{"-", "1"}]]}], 
     SubscriptBox["|", "26"]}]}], TraditionalForm]],ExpressionUUID->
  "97895c4b-1cf9-4163-87ce-0b6f8e5333a0"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"b", " ", "=", " ", 
    RowBox[{"|", 
     RowBox[{"H", "-", 
      RowBox[{"a", " ", "E"}]}], 
     SubscriptBox["|", "26"]}]}], TraditionalForm]],ExpressionUUID->
  "c0c24592-f32b-4b6d-a8b5-af73807ef25e"]
}], "Text",
 CellChangeTimes->{{3.6016975205300055`*^9, 3.6016975559200554`*^9}, {
  3.7908781594309683`*^9, 
  3.7908782213237734`*^9}},ExpressionUUID->"3a51bf12-814f-4d01-b5e6-\
6a97de11313b"],

